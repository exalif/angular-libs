{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-breadcrumbs": {
      "root": "projects/ngx-breadcrumbs",
      "sourceRoot": "projects/ngx-breadcrumbs/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-breadcrumbs/tsconfig.lib.json",
            "project": "projects/ngx-breadcrumbs/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-breadcrumbs/tsconfig.lib.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "angular-libs-demo": {
      "root": "projects/angular-libs-demo/",
      "sourceRoot": "projects/angular-libs-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-libs-demo",
            "index": "projects/angular-libs-demo/src/index.html",
            "main": "projects/angular-libs-demo/src/main.ts",
            "polyfills": "projects/angular-libs-demo/src/polyfills.ts",
            "tsConfig": "projects/angular-libs-demo/tsconfig.app.json",
            "assets": [
              "projects/angular-libs-demo/src/favicon.ico",
              "projects/angular-libs-demo/src/assets"
            ],
            "styles": [
              "projects/angular-libs-demo/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/angular-libs-demo/src/environments/environment.ts",
                  "with": "projects/angular-libs-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-libs-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-libs-demo:build:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/angular-libs-demo/tsconfig.app.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-test-utils": {
      "root": "projects/ngx-test-utils",
      "sourceRoot": "projects/ngx-test-utils/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-test-utils/tsconfig.lib.json",
            "project": "projects/ngx-test-utils/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-test-utils/tsconfig.lib.json",
              "projects/ngx-test-utils/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "angular-libs-demo"
}
