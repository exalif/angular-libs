    "ngx-breadcrumbs": {
      "root": "projects/ngx-breadcrumbs",
      "sourceRoot": "projects/ngx-breadcrumbs/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-breadcrumbs/tsconfig.lib.json",
            "project": "projects/ngx-breadcrumbs/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-breadcrumbs/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-breadcrumbs/**/*.ts",
              "projects/ngx-breadcrumbs/**/*.html"
            ]
          }
        }
      }
    },
