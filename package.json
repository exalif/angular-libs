{
  "name": "angular-libs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "concurrently --kill-others -s \"first\" \"npm run upload-server:dev\" \"npm run start\"",
    "build:demo": "ng build angular-libs-demo --prod",
    "build:ngx-breadcrumbs": "ng build ngx-breadcrumbs",
    "build:ngx-test-utils": "ng build ngx-test-utils",
    "build:ngx-file-upload": "ng build ngx-file-upload",
    "build:ngx-k-code": "ng build ngx-k-code",
    "test": "jest --colors  --coverage",
    "test:ci": "jest --colors --coverage --silent",
    "test:watch": "jest --watch --colors --verbose false",
    "lint": "ng lint",
    "upload-server": "node ./server/index.js",
    "upload-server:dev": "cd ./server && NODE_ENV=development nodemon ."
  },
  "dependencies": {
    "@angular/animations": "7.2.13",
    "@angular/common": "7.2.13",
    "@angular/compiler": "7.2.13",
    "@angular/core": "7.2.13",
    "@angular/forms": "7.2.13",
    "@angular/http": "7.2.13",
    "@angular/platform-browser": "7.2.13",
    "@angular/platform-browser-dynamic": "7.2.13",
    "@angular/router": "7.2.13",
    "core-js": "2.6.5",
    "rxjs": "6.4.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.8",
    "@angular-devkit/build-ng-packagr": "0.13.8",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.2.13",
    "@angular/language-service": "7.2.13",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "body-parser": "1.18.3",
    "bytes": "3.1.0",
    "codelyzer": "5.0.0",
    "concurrently": "4.1.0",
    "cors": "2.8.5",
    "coveralls": "3.0.4",
    "express": "4.16.4",
    "istanbul-reports": "2.1.1",
    "jest": "24.7.1",
