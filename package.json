    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/material": "14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "core-js": "2.6.8",
    "lodash.template": "4.5.0",
    "rxjs": "7.5.5",
    "signature_pad": "4.0.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0",
    "@angular/cli": "14.0.0",
    "@angular/compiler-cli": "14.0.0",
    "@angular/language-service": "14.0.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/signature_pad": "2.3.2",
    "body-parser": "1.19.2",
    "bytes": "3.1.2",
    "codelyzer": "6.0.2",
    "concurrently": "7.0.0",
    "cors": "2.8.5",
    "coveralls": "3.1.1",
    "express": "4.17.3",
    "istanbul-reports": "3.1.4",
    "jest": "27.5.1",
    "jest-haste-map": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "jest-resolve": "27.5.1",
    "mocha": "9.2.2",
    "mochawesome": "7.1.3",
    "morgan": "1.10.0",
    "ng-packagr": "14.0.0",
    "node-uploadx": "1.2.8",
    "nodemon": "2.0.15",
    "rollup": "1.32.1",
