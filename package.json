    "publish:ngx-mat-popover": "cd dist/ngx-mat-popover && npm publish . && cd ../..",
    "publish:all": "yarn publish:ngx-breadcrumbs && yarn publish:ngx-skeleton-loader && yarn publish:ngx-file-upload && yarn publish:ngx-k-code && yarn publish:ngx-mat-popover && yarn publish:ngx-signature-pad && yarn publish:ngx-keepalive"
  },
