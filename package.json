{
  "name": "angular-libs",
  "version": "0.0.0",
  "scripts": {
    "build:ngx-breadcrumbs": "ng build ngx-breadcrumbs --configuration production",
    "build:ngx-skeleton-loader": "ng build ngx-skeleton-loader --configuration production",
    "build:ngx-file-upload": "ng build ngx-file-upload --configuration production",
    "build:ngx-signature-pad": "ng build ngx-signature-pad --configuration production",
    "build:ngx-k-code": "ng build ngx-k-code --configuration production",
    "build:ngx-mat-popover": "ng build ngx-mat-popover --configuration production && scss-bundle -c projects/ngx-mat-popover/scss-bundle.config.json",
    "build:all": "yarn build:ngx-breadcrumbs && yarn build:ngx-skeleton-loader && yarn build:ngx-file-upload && yarn build:ngx-signature-pad && yarn build:ngx-k-code && yarn build:ngx-mat-popover",
    "test": "jest --colors  --coverage",
    "test:ci": "jest --colors --coverage --silent",
    "test:watch": "jest --watch --colors --verbose false",
    "lint": "ng lint",
    "publish:ngx-breadcrumbs": "cd dist/ngx-breadcrumbs && npm publish . && cd ../..",
    "publish:ngx-skeleton-loader": "cd dist/ngx-skeleton-loader && npm publish . && cd ../..",
    "publish:ngx-file-upload": "cd dist/ngx-file-upload && npm publish . && cd ../..",
    "publish:ngx-signature-pad": "cd dist/ngx-signature-pad && npm publish . && cd ../..",
    "publish:ngx-k-code": "cd dist/ngx-k-code && npm publish . && cd ../..",
    "publish:ngx-mat-popover": "cd dist/ngx-mat-popover && npm publish . && cd ../..",
    "publish:all": "yarn publish:ngx-breadcrumbs && yarn publish:ngx-skeleton-loader && yarn publish:ngx-file-upload && yarn publish:ngx-k-code && yarn publish:ngx-mat-popover && yarn publish:ngx-signature-pad"
  },
  "dependencies": {
    "@angular/animations": "14.0.0",
