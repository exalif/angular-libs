{
  "name": "angular-libs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "concurrently --kill-others -s \"first\" \"npm run upload-server:dev\" \"npm run start\"",
    "build:demo": "ng build angular-libs-demo --configuration production",
    "build:ngx-breadcrumbs": "ng build ngx-breadcrumbs --configuration production",
    "build:ngx-skeleton-loader": "ng build ngx-skeleton-loader --configuration production",
    "build:ngx-file-upload": "ng build ngx-file-upload --configuration production",
    "build:ngx-signature-pad": "ng build ngx-signature-pad --configuration production",
    "build:ngx-k-code": "ng build ngx-k-code --configuration production",
    "build:ngx-mat-popover": "ng build ngx-mat-popover --configuration production && scss-bundle -c projects/ngx-mat-popover/scss-bundle.config.json",
    "test": "jest --colors  --coverage",
    "test:ci": "jest --colors --coverage --silent",
    "test:watch": "jest --watch --colors --verbose false",
    "lint": "ng lint",
    "upload-server": "node ./server/index.js",
    "upload-server:dev": "cd ./server && NODE_ENV=development nodemon ."
  },
  "dependencies": {
    "@angular/animations": "13.3.0",
    "@angular/cdk": "13.3.1",
    "@angular/cdk-experimental": "13.3.1",
    "@angular/common": "13.3.0",
    "@angular/compiler": "13.3.0",
    "@angular/core": "13.3.0",
    "@angular/forms": "13.3.0",
    "@angular/material": "13.3.1",
    "@angular/platform-browser": "13.3.0",
    "@angular/platform-browser-dynamic": "13.3.0",
    "@angular/router": "13.3.0",
    "core-js": "2.6.8",
    "lodash.template": "4.5.0",
    "rxjs": "7.5.5",
    "signature_pad": "4.0.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.0",
    "@angular/cli": "13.3.0",
    "@angular/compiler-cli": "13.3.0",
    "@angular/language-service": "13.3.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.11.1",
    "@types/signature_pad": "2.3.0",
    "body-parser": "1.19.2",
    "bytes": "3.1.2",
    "codelyzer": "6.0.2",
    "concurrently": "7.0.0",
    "cors": "2.8.5",
    "coveralls": "3.1.1",
    "express": "4.17.3",
    "istanbul-reports": "3.1.4",
    "jest": "24.9.0",
    "jest-environment-jsdom-thirteen": "1.0.1",
    "jest-haste-map": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "jest-resolve": "24.9.0",
    "mocha": "6.2.1",
    "mochawesome": "4.1.0",
    "morgan": "1.10.0",
    "ng-packagr": "13.3.0",
    "node-uploadx": "1.2.8",
    "nodemon": "2.0.15",
    "rollup": "1.32.1",
    "scss-bundle": "3.1.2",
    "ts-node": "10.7.0",
    "tslint": "~6.1.3",
    "typescript": "4.6.3"
  }
}
