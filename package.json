    "jest-resolve": "24.7.1",
    "mocha": "6.1.3",
    "mochawesome": "3.1.1",
    "morgan": "1.9.1",
    "ng-packagr": "4.7.1",
    "nodemon": "1.18.11",
    "node-uploadx": "1.2.2",
    "ts-node": "7.0.1",
    "tsickle": "0.34.0",
    "tslib": "1.9.3",
    "tslint": "5.15.0",
    "typescript": "3.2.4"
