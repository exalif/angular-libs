{
  "name": "angular-libs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "concurrently --kill-others -s \"first\" \"npm run upload-server:dev\" \"npm run start\"",
    "build:demo": "ng build angular-libs-demo --prod",
    "build:ngx-breadcrumbs": "ng build ngx-breadcrumbs",
    "build:ngx-skeleton-loader": "ng build ngx-skeleton-loader",
    "build:ngx-file-upload": "ng build ngx-file-upload",
    "build:ngx-k-code": "ng build ngx-k-code",
    "build:ngx-mat-popover": "ng build ngx-mat-popover && scss-bundle -c projects/ngx-mat-popover/scss-bundle.config.json",
    "test": "jest --colors  --coverage",
    "test:ci": "jest --colors --coverage --silent",
    "test:watch": "jest --watch --colors --verbose false",
    "lint": "ng lint",
    "upload-server": "node ./server/index.js",
    "upload-server:dev": "cd ./server && NODE_ENV=development nodemon ."
  },
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/forms": "9.1.0",
    "@angular/material": "9.2.0",
    "@angular/cdk": "9.2.0",
    "@angular/cdk-experimental": "9.2.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "core-js": "2.6.8",
    "lodash.template": "4.5.0",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@types/jest": "24.0.18",
    "@types/node": "^12.11.1",
    "body-parser": "1.19.0",
    "bytes": "3.1.0",
    "codelyzer": "5.2.2",
    "concurrently": "5.1.0",
    "cors": "2.8.5",
    "coveralls": "3.0.11",
    "express": "4.17.1",
    "istanbul-reports": "2.2.6",
    "jest": "24.9.0",
    "jest-environment-jsdom-thirteen": "1.0.1",
    "jest-haste-map": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "jest-resolve": "24.9.0",
    "mocha": "6.2.1",
    "mochawesome": "4.1.0",
    "morgan": "1.10.0",
    "ng-packagr": "9.1.0",
    "nodemon": "2.0.2",
    "node-uploadx": "1.2.8",
    "rollup": "1.32.1",
    "scss-bundle": "3.1.1",
    "ts-node": "8.8.1",
    "tslint": "6.1.1",
    "typescript": "3.7.5"
  }
}
